{
  "name": "daysport-monorepo",
  "version": "1.0.0",
  "private": true,
  "description": "Daysport - E-commerce profesional",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd apps/backend && npm run dev",
    "dev:frontend": "cd apps/frontend && npm run dev",
    "build": "npm run build:backend && npm run build:frontend",
    "build:backend": "cd apps/backend && npm run build",
    "build:frontend": "cd apps/frontend && npm run build",
    "start": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
    "start:backend": "cd apps/backend && npm run start",
    "start:frontend": "cd apps/frontend && npm run start",
    "clean": "rm -rf node_modules apps/backend/node_modules apps/frontend/node_modules",
    "install:all": "npm install && cd apps/backend && npm install && cd ../frontend && npm install"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "keywords": ["ecommerce", "sports", "nestjs", "nextjs"],
  "author": "Daysport Team",
  "license": "MIT"
}